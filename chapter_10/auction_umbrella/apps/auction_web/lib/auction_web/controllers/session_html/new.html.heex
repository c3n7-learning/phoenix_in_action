<h1>Log In</h1>

<.form :let={f} for={@conn} action={~p"/users"} as={:user_params}>
  <div class="flex flex-col space-y-2">
    <.input name="username" value="" label="Username" />
    <.input name="password" value="" label="Password" type="password" />
  </div>
  <.button type="submit" class="mt-2">Submit</.button>
</.form>
